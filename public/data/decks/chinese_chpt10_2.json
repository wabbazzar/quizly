    "dialogues": {
      "dialogue_001": {
        "lines": [
          {
            "a": "你说中文说得真好。平常怎么准备上你的中文课?",
            "b": "Nǐ shuō zhōngwén shuō dé zhēn hǎo. Píngcháng zěnme zhǔnbèi shàng nǐ de zhōngwén kè?",
            "c": "You speak Chinese so well. Normally, how do you prepare for your Chinese class?",
            "wordAlignments": [
              {"chinese": "你", "pinyin": "Nǐ", "english": "You"},
              {"chinese": "说", "pinyin": "shuō", "english": "speak"},
              {"chinese": "中文", "pinyin": "zhōngwén", "english": "Chinese"},
              {"chinese": "说得", "pinyin": "shuō dé", "english": "so"},
              {"chinese": "真", "pinyin": "zhēn", "english": ""},
              {"chinese": "好", "pinyin": "hǎo", "english": "well"},
              {"chinese": "。", "pinyin": ".", "english": "."},
              {"chinese": "平常", "pinyin": "Píngcháng", "english": "Normally"},
              {"chinese": "怎么", "pinyin": "zěnme", "english": "how"},
              {"chinese": "准备", "pinyin": "zhǔnbèi", "english": "do you prepare"},
              {"chinese": "上", "pinyin": "shàng", "english": "for"},
              {"chinese": "你的", "pinyin": "nǐ de", "english": "your"},
              {"chinese": "中文", "pinyin": "zhōngwén", "english": "Chinese"},
              {"chinese": "课", "pinyin": "kè", "english": "class"},
              {"chinese": "?", "pinyin": "?", "english": "?"}
            ]
          },
          {
            "a": "我每天早上都听录音。",
            "b": "Wǒ měitiān zǎoshang dōu tīng lùyīn.",
            "c": "I listen to the audio every morning.",
            "wordAlignments": [
              {"chinese": "我", "pinyin": "Wǒ", "english": "I"},
              {"chinese": "每天", "pinyin": "měitiān", "english": "every"},
              {"chinese": "早上", "pinyin": "zǎoshang", "english": "morning"},
              {"chinese": "都", "pinyin": "dōu", "english": ""},
              {"chinese": "听", "pinyin": "tīng", "english": "listen to"},
              {"chinese": "录音", "pinyin": "lùyīn", "english": "the audio"},
              {"chinese": "。", "pinyin": ".", "english": "."}
            ]
          }
        ]
      },
      "dialogue_002": {
        "lines": [
          {
            "a": "我想给我的朋友发短信。",
            "b": "Wǒ xiǎng gěi wǒ de péngyǒu fā duǎnxìn.",
            "c": "I want to send a text message to my friend.",
            "wordAlignments": [
              {"chinese": "我", "pinyin": "Wǒ", "english": "I"},
              {"chinese": "想", "pinyin": "xiǎng", "english": "want"},
              {"chinese": "给", "pinyin": "gěi", "english": "to"},
              {"chinese": "我的", "pinyin": "wǒ de", "english": "my"},
              {"chinese": "朋友", "pinyin": "péngyǒu", "english": "friend"},
              {"chinese": "发", "pinyin": "fā", "english": "send"},
              {"chinese": "短信", "pinyin": "duǎnxìn", "english": "a text message"},
              {"chinese": "。", "pinyin": ".", "english": "."}
            ]
          },
          {
            "a": "你可以用手机发短信。",
            "b": "Nǐ kěyǐ yòng shǒujī fā duǎnxìn.",
            "c": "You can use your cell phone to send a text.",
            "wordAlignments": [
              {"chinese": "你", "pinyin": "Nǐ", "english": "You"},
              {"chinese": "可以", "pinyin": "kěyǐ", "english": "can"},
              {"chinese": "用", "pinyin": "yòng", "english": "use"},
              {"chinese": "手机", "pinyin": "shǒujī", "english": "your cell phone"},
              {"chinese": "发", "pinyin": "fā", "english": "to send"},
              {"chinese": "短信", "pinyin": "duǎnxìn", "english": "a text"},
              {"chinese": "。", "pinyin": ".", "english": "."}
            ]
          }
        ]
      }
